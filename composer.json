{
  "name": "mehrwert/fal-quota",
  "description": "FAL Quota for TYPO3",
  "type": "typo3-cms-extension",
  "license": "GPL-3.0-or-later",
  "prefer-stable": true,
  "authors": [
    {
      "name": "mehrwert intermediale kommunikation GmbH",
      "email": "typo3@mehrwert.de",
      "role": "Developer"
    }
  ],
  "homepage": "https://extensions.typo3.org/extension/fal_quota/",
  "support": {
    "issues": "https://github.com/mehrwert/TYPO3-FAL-Quota/issues",
    "source": "https://github.com/mehrwert/TYPO3-FAL-Quota"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "phpstan/extension-installer": true,
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "require": {
    "php": "^8.3",
    "psr/event-dispatcher": "^1.0",
    "psr/http-message": "^2.0",
    "psr/log": "^3.0",
    "symfony/console": "^7.3",
    "symfony/mime": "^7.3",
    "typo3/cms-backend": "^12.4",
    "typo3/cms-core": "^12.4",
    "typo3/cms-extbase": "^12.4",
    "typo3/cms-extensionmanager": "^12.4",
    "typo3/cms-filelist": "^12.4",
    "typo3/cms-fluid": "^12.4",
    "typo3/cms-install": "^12.4",
    "typo3/cms-scheduler": "^12.4",
    "typo3fluid/fluid": "^2.15"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "networkteam/sentry-client": "^5.2",
    "phpstan/extension-installer": "^1.3",
    "rector/rector": "^1.1",
    "roave/security-advisories": "dev-latest",
    "saschaegerer/phpstan-typo3": "^1.10",
    "ssch/typo3-rector": "^2.14",
    "typo3/cms-beuser": "^12.4",
    "typo3/cms-frontend": "^12.4",
    "typo3/cms-lowlevel": "^12.4"
  },
  "suggest": {
    "ext-intl": "*"
  },
  "autoload": {
    "psr-4": {
      "Mehrwert\\FalQuota\\": "Classes/"
    }
  },
  "scripts": {
    "cs:analyze": [
      "phpstan analyze --memory-limit=512M --verbose --configuration build/phpstan.neon"
    ],
    "cs:baseline": [
      "phpstan analyze --memory-limit=512M --verbose --configuration build/phpstan.neon --generate-baseline build/phpstan-baseline.neon"
    ],
    "cs:fix": [
      "@putenv PHP_CS_FIXER_IGNORE_ENV=1",
      "php-cs-fixer fix --config=build/php-cs-fixer.php --verbose --using-cache=no --diff --path-mode=intersection ."
    ],
    "test:php:lint": [
      "php -l *.php $(find Classes -iname '*.php') $(find Configuration -iname '*.php')"
    ]
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "fal_quota",
      "web-dir": ".Build/web"
    }
  }
}
